# Quit BSPWM
super + shift + l
    bspc quit

super + l
    ${HOME}/bin/xlock.sh

super + r
    rofi -show run

super + Tab
    rofi -show window

super + F1
    xterm

super + F2
    firefox

super + F3
    emacs

super + F4
    xterm -e mc

# Workstation Monitor
super + Right
    bspc desktop -f next

super + Left
    bspc desktop -f prev

# Window
ctrl + q
    bspc window -c

# Audio
super + Up
    ${HOME}/bin/sndvol +

super + Down
    ${HOME}/bin/sndvol -
	
# Screenshot
Print
    xfce4-screenshooter
