"XFCE"
"Accessibility" "preferences-desktop-accessibility" exec "xfce4-accessibility-settings"
"Appearance" "preferences-desktop-theme" exec "xfce4-appearance-settings"
"Desktop" "preferences-desktop-wallpaper" exec "xfdesktop-settings"
"Display" "video-display" exec "xfce4-display-settings"
"File Manager" "system-file-manager" exec "thunar-settings"
"Keyboard" "preferences-desktop-keyboard" exec "xfce4-keyboard-settings"
"MIME Type Editor" "/usr/share/icons/Adwaita/48x48/mimetypes/application-x-executable.png" exec "xfce4-mime-settings"
"Mouse and Touchpad" "preferences-desktop-peripherals" exec "xfce4-mouse-settings"
"Panel" "/usr/share/icons/hicolor/48x48/apps/xfce4-panel.png" exec "xfce4-panel --preferences"
"Preferred Applications" "/usr/share/icons/hicolor/48x48/apps/preferences-desktop-default-applications.png" exec "exo-preferred-applications"
"Removable Drives and Media" "/usr/share/icons/Adwaita/48x48/devices/drive-removable-media.png" exec "thunar-volman-settings"
"Session and Startup" "/usr/share/icons/hicolor/48x48/apps/xfce4-session.png" exec "xfce4-session-settings"
"Settings Editor" "preferences-system" exec "xfce4-settings-editor"
"Window Manager Tweaks" "/usr/share/icons/hicolor/48x48/apps/wmtweaks.png" exec "xfwm4-tweaks-settings"
"Window Manager" "/usr/share/icons/hicolor/48x48/apps/xfwm4.png" exec "xfwm4-settings"
"Workspaces" "/usr/share/icons/hicolor/48x48/apps/xfce4-workspaces.png" exec "xfwm4-workspace-settings"
"Xfce Terminal" "utilities-terminal" exec "xfce4-terminal --preferences"
